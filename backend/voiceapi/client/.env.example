# Vite Environment Variables Example
# Copy this file to .env and update the values as needed

# API Base URL - The backend server URL
VITE_API_BASE_URL=http://localhost:3000

# Default User ID - The user identifier for API requests
VITE_DEFAULT_USER_ID=test-user-123

# Auth Token - Bearer token for API authentication
VITE_AUTH_TOKEN=Bearer your-auth-token-here

# LLM API Base URL - The LLM API server URL
VITE_LLM_API_BASE_URL=http://localhost:3002

# VAPI Configuration - Voice AI Platform Integration
VITE_VAPI_API_KEY=your_vapi_public_api_key_here
VITE_VAPI_ASSISTANT_ID=your_vapi_assistant_id_here
